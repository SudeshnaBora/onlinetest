CREATE TABLE online_test
( 	row_id INT(20) NOT NULL AUTO_INCREMENT,
	success VARCHAR(6),
	test_part VARCHAR(50),
	trial_type VARCHAR(50),
	trial_index INT(20),
	time_elapsed BIGINT(50),
	internal_node_id VARCHAR(50),
	subject VARCHAR(50),
	PRIMARY KEY (row_id, subject),
	browser_name VARCHAR(50),
	browser_version VARCHAR(50),
	os_name VARCHAR(50),
	os_version VARCHAR(50),
	screen_resolution VARCHAR(50),
	window_resolution VARCHAR(50),
	rt DECIMAL(50),
	stimulus VARCHAR(50),
	key_press INT(20),
	button_pressed VARCHAR(50),
	responses VARCHAR(50),
	question_order VARCHAR(50),
	correct_answer VARCHAR(50),
	name VARCHAR(50),
	correct VARCHAR(6),
	choices VARCHAR(50),
	correct_choice VARCHAR(50),
	trial_duration INT(20),
	response_ends_trial VARCHAR(6),
	number_of_apertures INT(20),
	number_of_dots INT(20),
	number_of_sets INT(20),
	coherent_direction INT(20),
	coherence DECIMAL(50),opposite_coherence INT(20),
	dot_radius INT(20),
	dot_life INT(20),
	move_distance INT(20),aperture_width INT(20),
	aperture_height INT(20),
	dot_color VARCHAR(50),
	background_color VARCHAR(50),
	RDK_type INT(20),
	aperture_type INT(20),
	reinsert_type INT(20),
	frame_rate DECIMAL(50),
	frame_rate_array VARCHAR(50),
	number_of_frames INT(20),
	aperture_center_x INT(20),
	aperture_center_y INT(20),
	fixation_cross VARCHAR(6),
	fixation_cross_width INT(20),
	fixation_cross_height INT(20),
	fixation_cross_color VARCHAR(50),
	fixation_cross_thickness INT(20),
	border VARCHAR(6),
	border_thickness INT(20),
	border_color INT(20),
	canvas_width INT(20),
	canvas_height INT(20),
	trial_number INT(20),
	block_type INT(20),
	run INT(20),
	RCat INT(20)
);




